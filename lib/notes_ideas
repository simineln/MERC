region do
  name: string


subregion do
  name: string
  region_id: integer


meter do
  profile: string
  name: string
  number: integer
  account: string
  region_id: integer
  type: integer
  kt: integer
  inverted: boolean
  subaccount: string


reading do
  meter_id: integer
  date: datetime
  ad_p: float
  ad_m: float
  rd_p: float
  rd_m: float
  aec_p: float
  aec_m: float
  rec_p: float
  rec_m: float
  vph1: float
  vph2: float
  vph3: float
  iph1: float
  iph2: float
  iph3: float
  cosPHI: float
  THD_U: float
  THD_I: float


:meter_id, :date, :ad_p, :ad_m, :rd_p, :rd_m, :aec_p, :aec_m, :rec_p, :rec_m, :vph1, :vph2, :vph3, :iph1, :iph2, :iph3, :cosPHI, :THD_U, :THD_I